{
  "version": 2,
  "builds": [
    {
      "src": "docs-site/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "docs-site/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd docs-site && pnpm run build:vercel",
  "outputDirectory": "docs-site/build",
  "installCommand": "cd docs-site && pnpm install",
  "framework": null
}
